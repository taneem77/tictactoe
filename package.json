{
	"name": "tictactoe",
	"displayName": "TicTacToe",
	"description": "Play TicTacToe inside VSCode while your code compiles",
	"version": "0.0.2",
	"publisher": "srujandeshpande",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/srujandeshpande/tictactoe"
	},
	"icon": "assets/icon.png",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tictactoe.start"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tictactoe.start",
				"title": "Start a new TicTacToe game",
				"category": "TicTacToe"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "node --openssl-legacy-provider ./node_modules/webpack/bin/webpack.js",
		"watch": "node --openssl-legacy-provider ./node_modules/webpack/bin/webpack.js --mode none --watch",
		"package": "node --openssl-legacy-provider ./node_modules/webpack/bin/webpack.js --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^8.2.3",
		"@types/node": "^12.20.55",
		"@types/vscode": "^1.56.0",
		"@types/webpack-env": "^1.18.8",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"ts-loader": "^9.5.4",
		"typescript": "^5.9.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	}
}
